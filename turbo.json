{
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "test:unit": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "test:watch": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "test:cov": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "test:debug": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "test:e2e": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "deps:check": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "start:dev": {
      "cache": false,
      "outputs": [
        "dist/**"
      ]
    },
    "start:debug": {
      "cache": false,
      "outputs": [
        "dist/**"
      ]
    }
  }
}